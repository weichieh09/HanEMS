@readOnly
entity ItemView {
	id Long,
	name String,
	total String,
	ready String,
	working String,
	canNot String,
}

@readOnly
entity EqReturnView {
	id Long,
	idno String,
	name String,
	itemId Long,
	itemName String,
	statusId Long,
	statusName String,
	personId Long,
	personName String,
}

filter *
dto * with mapstruct
paginate * with pagination
service * with serviceClass
