@readOnly
entity ItemView {
	id Long,
	name String,
	total String,
	ready String,
	working String,
	canNot String,
}

@readOnly
entity EqReturnView {
	id Long,
	idno String,
	name String,
	itemId Long,
	statusId Long,
	personId Long,
}

filter *
dto * with mapstruct
paginate * with pagination
service * with serviceClass
